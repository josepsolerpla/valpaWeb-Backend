enum Role {
    ROOT
    ADMIN
    USER
  }

type User {
  id: ID! @unique
  name: String! @unique
  password: String!
  email: String! @unique
  role: Role! @default(value: USER)
  createdAt: DateTime!
  updatedAt: DateTime!
	slug: String
	image: String
}

type Authed {
  id: ID! @unique
  tokenGoogle: String! @unique
}

type AllowedCalendar {
  id: ID! @unique
  idCalendar: String @unique
  createdAt: DateTime!
}